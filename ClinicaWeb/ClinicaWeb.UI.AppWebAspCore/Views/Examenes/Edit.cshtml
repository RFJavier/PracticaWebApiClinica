@model ClinicaWeb.EntidadesDeNegocio.Examenes

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>Examenes</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="Examen" class="control-label"></label>
                <input asp-for="Examen" class="form-control" />
                <span asp-validation-for="Examen" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Guardar Cambios" class="btn btn-primary" />
            </div>
            <span class="text-danger">@ViewBag.Error</span>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index" class="btn btn-dark">Regresar a la Lista</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
